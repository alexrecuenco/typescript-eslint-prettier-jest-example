{
    "name": "typescript-eslint-prettier-jest-example",
    "version": "0.1.0",
    "description": "Example of Combining eslint prettier jest.",
    "main": "./build/bin/cli.js",
    "types": "./build/src/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/alexrecuenco/typescript-eslint-prettier-jest-example.git"
    },
    "bin": "./build/bin/cli.js",
    "scripts": {
        "start": "npm run build && node .",
        "build": "npm run build:debug",
        "build:debug": "tsc",
        "build:prod": "tsc --build tsconfig.prod.json",
        "watch": "tsc-watch --onSuccess 'node .'",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
        "test": "jest"
    },
    "license": "SEE LICENSE IN LICENSE",
    "keywords": [
        "jest",
        "test",
        "datetime",
        "internationalization"
    ],
    "devDependencies": {
        "@jest/types": "^26.6.2",
        "@types/jest": "^26.0.22",
        "@types/node": "^14.14.37",
        "@typescript-eslint/eslint-plugin": "^4.20.0",
        "@typescript-eslint/parser": "^4.20.0",
        "eslint": "^7.23.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.3.3",
        "eslint-plugin-prettier": "^3.3.1",
        "fast-check": "^2.14.0",
        "jest": "^26.6.3",
        "prettier": "2.2.1",
        "supports-color": "^8.1.1",
        "ts-jest": "^26.5.4",
        "tsc-watch": "^4.2.9",
        "typescript": "^4.2.3"
    }
}
