{
    "name": "typescript-eslint-prettier-jest-example",
    "version": "0.1.0",
    "description": "Example of Combining eslint prettier jest.",
    "repository": {
        "type": "git",
        "url": "https://github.com/alexrecuenco/typescript-eslint-prettier-jest-example.git"
    },
    "engines": {
        "npm": ">=9.0.0",
        "node": ">=18.0.0 <21.0.0"
    },
    "scripts": {
        "start": "npm run build && node .",
        "build": "npm run build:debug",
        "build:debug": "tsc",
        "build:prod": "tsc --build tsconfig.prod.json",
        "watch": "tsc-watch --onSuccess 'node .'",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier . --check",
        "format:fix": "prettier . -w",
        "test": "jest",
        "deps:upgrade": "npm-check -u"
    },
    "license": "SEE LICENSE IN LICENSE",
    "keywords": [
        "jest",
        "test",
        "datetime",
        "internationalization"
    ],
    "devDependencies": {
        "@eslint/js": "^8.57.0",
        "@types/jest": "^29.5.3",
        "@types/node": "^20.11.25",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "@typescript-eslint/parser": "^7.1.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jest": "^27.2.3",
        "fast-check": "^3.12.0",
        "globals": "^14.0.0",
        "jest": "^29.6.2",
        "prettier": "^3.2.5",
        "ts-jest": "^29.1.1",
        "tsc-watch": "^6.0.4",
        "typescript": "^5.3.2",
        "typescript-eslint": "^7.1.1"
    }
}
