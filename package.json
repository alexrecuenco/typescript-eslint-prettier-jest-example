{
    "name": "typescript-eslint-prettier-jest-example",
    "version": "0.0.1",
    "description": "Example of Combining eslint prettier jest.",
    "main": "./build/bin/cli.js",
    "types": "./build/src/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/alexrecuenco/typescript-eslint-prettier-jest-example.git"
    },
    "bin": "./build/bin/cli.js",
    "scripts": {
        "start": "npm run build && node .",
        "build": "npm run build:debug",
        "build:debug": "tsc",
        "build:prod": "tsc --build tsconfig.prod.json",
        "watch": "tsc-watch --onSuccess 'node .'",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
        "test": "jest"
    },
    "license": "SEE LICENSE IN LICENSE",
    "keywords": [
        "jest",
        "test",
        "datetime",
        "internationalization"
    ],
    "dependencies": {},
    "devDependencies": {
        "@jest/types": "^26.0.1",
        "@types/jest": "^25.2.1",
        "@types/node": "^13.13.4",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jest": "^23.8.2",
        "eslint-plugin-prettier": "^3.1.3",
        "fast-check": "^1.24.2",
        "jest": "^25.5.4",
        "prettier": "2.0.5",
        "supports-color": "^7.1.0",
        "ts-jest": "^25.4.0",
        "tsc-watch": "^4.2.3",
        "typescript": "^3.8.3"
    }
}
