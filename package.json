{
    "name": "react-frontend-express-backend-multiroot",
    "version": "0.0.0",
    "type": "module",
    "description": "Example of Combining eslint prettier jest.",
    "repository": {
        "type": "git",
        "url": "https://github.com/alexrecuenco/react-frontend-express-backend-multiroot.git"
    },
    "engines": {
        "npm": ">=10.0.0",
        "node": ">=22.0.0 <23.0.0"
    },
    "private": "true",
    "workspaces": [
        "packages/interface",
        "packages/frontend",
        "packages/backend"
    ],
    "scripts": {
        "dev": "concurrently -k -n backend,frontend -c magenta,blue \"npm run dev -w backend\" \"npm run dev -w frontend\"",
        "test": "npm run test -ws --if-present",
        "start:all": "echo REDO npm run build && node . serve",
        "watch": "echo REDO tsc-watch --onSuccess 'node .'",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier . --check",
        "format:fix": "prettier . -w",
        "check:builds": "npm run build:prod -ws --if-present && npm run build:debug --ws --if-present",
        "check:lint": "npm run lint && npm run format",
        "check:all": "npm run check:lint && npm run check:builds",
        "deps:upgrade": "npm-check -u"
    },
    "license": "SEE LICENSE IN LICENSE",
    "keywords": [
        "jest",
        "test",
        "datetime",
        "internationalization"
    ],
    "devDependencies": {
        "@eslint/js": "^9.12.0",
        "backend": "^1.0.0",
        "concurrently": "^9.0.1",
        "eslint": "^9.12.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jest": "^28.8.3",
        "frontend": "^1.0.0",
        "glob": "^11.0.0",
        "globals": "^15.10.0",
        "interface": "^1.0.0",
        "prettier": "^3.3.3",
        "prettier-plugin-jsdoc": "^1.3.0",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.8.0"
    },
    "//": "typescript 5.6 not supported by typescript-estree"
}
